<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - مكتب المحاماه</title>
    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />*@
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/LawyerProject.styles.css" asp-append-version="true" />
    <link rel="icon" href="/favicon.ico" />
    @RenderSection("Css",false)
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">
                    <img src="/images/51-JK3hf1uL._AC_UF1000,1000_QL80_.jpg" alt="إدارة مكتب المحامي" width="60" height="50">

                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark my-link" asp-area="" asp-controller="Home" asp-action="Index">الصفحة الرئيسية</a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link text-dark my-link" asp-area="" asp-controller="City" asp-action="Index">المحافظات</a>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" href="#" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                المحاكم
                            </a>
                            <ul class="dropdown-menu dropdown-menu text-dark" aria-labelledby="navbarDarkDropdownMenuLink">
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="Court" asp-action="Index">المحاكم</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="Court" asp-action="Create">إضافة محكمة</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="CourtKind" asp-action="Index">انواع المحاكم</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="CourtKind" asp-action="Create">إضافة نوع محكمة</a></li>
                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" href="#" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                الموكلين
                            </a>
                            <ul class="dropdown-menu dropdown-menu text-dark" aria-labelledby="navbarDarkDropdownMenuLink">
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="Client" asp-action="Index">الموكلين</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="Client" asp-action="Create">إضافة موكل</a></li>
                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" href="#" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                الخصوم
                            </a>
                            <ul class="dropdown-menu dropdown-menu text-dark" aria-labelledby="navbarDarkDropdownMenuLink">
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="Enemy" asp-action="Index">الخصوم</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="Enemy" asp-action="Create">إضافة خصم</a></li>
                            </ul>
                        </li>

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" href="#" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                مكاتب التوثيق
                            </a>
                            <ul class="dropdown-menu dropdown-menu text-dark" aria-labelledby="navbarDarkDropdownMenuLink">
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="MaktabTawseek" asp-action="Index">مكاتب التوثيق</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="MaktabTawseek" asp-action="Create">إضافة مكاتب التوثيق</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="Tawkeel" asp-action="Index">التوكيلات</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="Tawkeel" asp-action="Create">إضافة توكيل</a></li>
                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" href="#" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                نيابات
                            </a>
                            <ul class="dropdown-menu dropdown-menu text-dark" aria-labelledby="navbarDarkDropdownMenuLink">
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="NyabaKind" asp-action="Index">انواع النيابات</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="NyabaKind" asp-action="Create">إضافة انواع النيابات</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="Nyaba" asp-action="Index">النيابات</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="Nyaba" asp-action="Create">إضافة نيابة</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="Dayra" asp-action="Index">الدوائر</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="Dayra" asp-action="Create">إضافة دائرة</a></li>
                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" href="#" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                الاعمال الإدارية
                            </a>
                            <ul class="dropdown-menu dropdown-menu text-dark" aria-labelledby="navbarDarkDropdownMenuLink">
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="AdministrativeWorkKind" asp-action="Index">انواع الأعمال الإدارية</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="AdministrativeWorkKind" asp-action="Create">إضافة انواع الأعمال الإدارية</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="AdministrativeWork" asp-action="Index">الأعمال الإدارية</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="AdministrativeWork" asp-action="Create">إضافة  الأعمال الإدارية</a></li>
                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" href="#" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                القضايا
                            </a>
                            <ul class="dropdown-menu dropdown-menu text-dark" aria-labelledby="navbarDarkDropdownMenuLink">
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="CaseKind" asp-action="Index">انواع القضايا</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="CaseKind" asp-action="Create">إضافة انواع القضايا</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="CaseSubKind" asp-action="Index">انواع الدعاوى</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="CaseSubKind" asp-action="Create">إضافة انواع الدعاوى</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="Case" asp-action="Index">القضايا</a></li>
                                <li><a class="nav-link text-dark my-link" asp-area="" asp-controller="Case" asp-action="Create">إضافة قضية</a></li>

                            </ul>
                        </li>
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; @DateTime.Now.Year - مكتب المحاماه - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
